{
  "name": "strangeluv",
  "version": "0.0.0",
  "description": "How I Learned to Stop Worrying and Love React",
  "main": "server/plugin.js",
  "engines": {
    "node": ">=6",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:src": "eslint src",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server",
      "env": {
        "NODE_ENV": "dev",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "dev",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start config/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigRoomStudios/strangeluv.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.13.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.20.0",
    "better-npm-run": "0.0.15",
    "css-loader": "^0.28.0",
    "cssnano": "^3.8.2",
    "debug": "^2.4.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.0",
    "fs-extra": "^2.0.0",
    "hapi": "^16.1.1",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.7.1",
    "inert": "^4.0.0",
    "keymirror": "^0.1.1",
    "labbable": "^2.1.1",
    "node-sass": "^4.5.0",
    "normalize.css": "^6.0.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^2.2.0",
    "react-router-redux": "^4.0.0",
    "redbox-react": "^1.2.2",
    "redux": "^3.0.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.3",
    "strangeluv-core": "^1.2.0",
    "style-loader": "^0.16.0",
    "url-loader": "^0.5.6",
    "webpack": "^2.0.0",
    "webpack-hot-middleware": "^2.11.0",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "codecov": "^2.1.0",
    "enzyme": "^2.5.0",
    "eslint": "^3.1.1",
    "eslint-config-hapi": "^10.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-react": "^6.0.0",
    "isparta-loader": "^2.0.0",
