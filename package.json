    "chai-enzyme": "v1.0.0-beta.0",
    "cheerio": "^0.22.0",
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-hapi": "^10.0.0",
