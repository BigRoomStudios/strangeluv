    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^3.0.0",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.13",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.0.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0"
  }
}
